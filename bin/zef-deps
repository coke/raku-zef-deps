#!/usr/bin/env raku

=begin comment

Use zef tool to identify all (nested) dependencies for a named module

=end comment

use App::Zef-Deps;

multi MAIN('.', Bool :$png=False, Bool :$json=False, Bool :$inverse=False)  {
    print MAIN-handler-dot(:$png, :$json, :$inverse);
    print "\n" if $json | $png;
}

multi MAIN(*@module, Bool :$png=False, Bool :$json=False, Bool :$inverse=False)  {
    print MAIN-handler(@module, :$png, :$json, :$inverse);
    print "\n" if $json | $png;
}

